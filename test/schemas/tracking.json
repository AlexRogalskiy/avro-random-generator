            "type": {
              "type": "string",
              "arg.options": [
                "English",
                "German",
                "Japanese"
              ]
            },
            "doc": "Accept-LanguageheaderfieldfromtheHTTPrequest."
          },
          {
            "name": "referer",
            "type": {
              "type": "string",
              "arg.regex": "(www\\.)?[a-z][a-z0-9]{0,9}\\.(com|org|net)"
            },
            "doc": "Referer(sic)headerfieldfromtheHTTPrequest."
          },
          {
            "name": "userAgent",
            "type": {
              "type": "string",
