          {
            "name": "merchantId",
            "type": [
              "null",
              "string"
            ],
            "doc": "ThemerchantID,ifitexistsunderthecontextofthistrackingevent.",
            "default": null
          },
          {
            "name": "userId",
            "type": [
              "null",
              "string"
            ],
            "doc": "TheuserID,ifitexistsunderthecontextofthistrackingevent.",
            "default": null
          }
        ]
      }
    },
    {
      "name": "details",
      "type": {
        "type": "map",
        "values": "string",
        "arg.length": {
          "min": 0,
          "max": 4
        }
      },
